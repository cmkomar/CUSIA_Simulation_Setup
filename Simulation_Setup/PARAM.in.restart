#ECHO
T

#INCLUDE
RESTART.in

#DESCRIPTION
Restart CUSIA event with time accurate mode on and two-way coupling of CIMI

#COMPONENTMAP
GM 0 -1 1
IE 0  1 1
IM 0  11 0

#TIMEACCURATE
T			DoTimeAccurate

STARTTIME
2012			iYear
10			iMonth
08			iDay
01			iHour
00			iMinute
00			iSecond
0.0			FracSecond

#ENDTIME            
2012	    		year
10              	month
09              	day
12		        hour
00             		minute
00              	second
0.0		        FracSecond

#PROGRESS
10			DnProgressShort
100			DnProgressLong

#SAVERESTART
T			DoSaveRestart
-1			DnSaveRestart
600.0			DtSaveRestart

#CHECKSTOP
T			DoCheckStop
-1			DnCheckStop
300.0			DtCheckStop

#CPUTIMEMAX
85000			!Max is 24:00

#COMPONENT
IM			NameComp
T			UseComp

#COUPLE2
GM			NameComp1
IE			NameComp2
-1			DnCouple
5.0			DtCouple

#COUPLE2
GM			NameSource
IM			NameTarget
-1			DnCouple
10.0			DtCouple

#COUPLE1
IE			NameSource
IM			NameTarget
-1			DnCouple
10.0			DtCouple

#CHECKTIMESTEP
F               DoCheckTimeStep
2               DnCheckTimeStep
1e-6            TimeStepMin

#BEGIN_COMP GM ---------------------------------------------------------------

#INCLUDE
GM/restartIN/restart.H

#INCLUDE
SAT_MASTER.in

#NONCONSERVATIVE
T                       UseNonConservative

#CONSERVATIVECRITERIA
1			nConservCrit
parabola		TypeConservCrit_I
 5.0			xParabolaConserv
15.0			yParabolaConserv

#SCHEME
2			nOrder
Rusanov			TypeFlux
mc3			TypeLimiter
1.2			LimiterBeta

#TIMESTEPPING
2			nStage
0.85			CflExpl

#BORIS
T
0.05

#OUTERBOUNDARY
outflow                 TypeBC_I(east_)
vary                    TypeBC_I(west_)
float                   TypeBC_I(south_)
float                   TypeBC_I(north_)
float                   TypeBC_I(bot_)
float                   TypeBC_I(top_)

#INNERBOUNDARY
ionosphere

#SOLARWINDFILE
T                       UseSolarWindFile (rest of parameters read if true)
IMF.dat                 NameSolarWindFile

#SAVELOGFILE
T			DoSaveLogfile
VAR			StringLogfile
-1			DnSaveLogfile
1.0			DtSaveLogfile
dt rho mx my mz p bx by bz pmin pmax dst dstdivb dstflx
3.0 3.5 5.0		StringRFlux

#IMCOUPLING
10.0			TauCoupleIM
T			DoImSatTrace
T			DoCoupleImPressure
T			DoCoupleImDensity
1e-4			DensityCoupleFloor
F			DoFixPolarRegion

! Pressures below pMinDim are replaced with pMinDim.
#MINIMUMPRESSURE
1e-5			pMinDim [nPa]
-1.0			pMinDim for fluid 2
2e-5			PeMinDim for electron pressure

! Density below DenMinDim are replaced with RhoMinDim.
#MINIMUMDENSITY
1e-4			RhoMinDim

#ANISOPRESSUREIM
T			DoAnisoPressureIMCoupling

#SAVEPLOT
4			nPlotFile
3d FUL idl		StringPlot
-1			DnSavePlot
1800.			DtSavePlot
-1.			DxSavePlot
y=0 FUL idl		StringPlot
-1			DnSavePlot
60.			DtSavePlot
-1.			DxSavePlot
z=0 FUL idl		StringPlot
-1			DnSavePlot
60.			DtSavePlot
-1.			DxSavePlot
1d FUL idl		StringPlot
-1			DnSavePlot
60.			DtSavePlot
-1.			DxSavePlot

#MAGNETOMETER
mags_all.dat
single                  TypeFileOut
-1                      DnOutput
10.0			DtOutput

#MAGNETOMETERGRID
real8			TypeFileMagGrid
GEO			TypeCoordMagGrid
72			nLonMagGrid
34			nLatMagGrid
   0.0			LonMinMagGrid
 360.0			LonMaxMagGrid
 -85.0			LatMinMagGrid
  85.0			LatMaxMagGrid
  -1			DnSaveMagGrid
  60.0			DtSaveMagGrid

#END_COMP GM -----------------------------------------------------------------

#BEGIN_COMP IE ---------------------------------------------------------------

#SAVEPLOT
1			nPlotFile
max idl                 StringPlot
-1			DnSavePlot
60.0			DtSavePlot

#SPS
T			UseSPS

#DEBUG
0			iDebugLevel
0			iDebugProc

#END_COMP IE -----------------------------------------------------------------

#BEGIN_COMP IM ---------------------------------------------------------------

#INCLUDE
IM/restartIN/restart.H

#RESTART
T			IsRestart
T			ReadRestartSat

INITIALF2
F			IsEmptyInitial
T			IsGmInitial
F			IsDataInitial
F			IsRBSPData

! Number density fractions for H+ and O+. Has to add up to 1.0
#COMPOSITION
FIXED			NameCompModel
0.8			Fraction_H+
0.2			Fraction_O+
1.0			Fraction_e-

! Decay for CIMI particle densities
#DECAY
T 			UseDecay
36000.			DecayTimescale in seconds

#STRONGDIFFUSION
F

#BMODEL
MHD			NameModel

#IEMODEL
F			UseWeimer

#TYPEBOUNDARY
Circle			TypeBoundary

#PLASMASHEET
F			UseYoungEtAl
F			UseBoundaryEbihara

#PRERUNFIELD
T			DoWritePrerun
F			UsePrerun
0.0			DtRead

#PRERUNSAT
T			DoWritePrerunSat
F			UsePrerunSat
0.0			DtReadSat

#SAVELOG
60.0			DtLogOut

#SAVEPLOT
3			nCIMIPlotType
2d eq			StringPlot
60.			DtOutput
fls all			StringPlot
60.			DtOutput
T			DoSaveSeparateFiles
psd e			StringPlot
60.			DtOutput
T			DoSaveSeparateFiles

#MINIMUMPRESSURETOGM
1e-5			MinimumPressureToGM

#IMTIMESTEP
10.0			IMDt
10.0			IMDtMax

#SETRB
12.			rb [R_E]

#SETBOUNDARYPARAMS
 4.0			DeltaRmax
10.0			DeltaMLTmax

#DTSATOUT
10.0			DtSatOut

#END_COMP IM -----------------------------------------------------------------

#END
